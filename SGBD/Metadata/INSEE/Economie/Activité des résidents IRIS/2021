COMMENT ON TABLE economie.activite_residents_2021 IS 'Voir métadonnées dans SGBD/Metadata/INSEE/Economie/Activité des résidents IRIS/2021';

CREATE SCHEMA IF NOT EXISTS admin;

CREATE TABLE IF NOT EXISTS admin.metadata_links (
    id SERIAL PRIMARY KEY,
    schema_name VARCHAR(100) NOT NULL,
    table_name VARCHAR(100) NOT NULL,
    metadata_path VARCHAR(255) NOT NULL,
    last_sync TIMESTAMP DEFAULT CURRENT_TIMESTAMP,
    UNIQUE(schema_name, table_name)
);

COMMENT ON TABLE admin.metadata_links IS 'Table de correspondance entre les tables PostgreSQL et leurs fichiers de métadonnées';

CREATE OR REPLACE FUNCTION admin.sync_table_comments() 
RETURNS INTEGER AS $$
DECLARE
    counter INTEGER := 0;
    r RECORD;
BEGIN
    FOR r IN 
        SELECT schema_name, table_name, metadata_path 
        FROM admin.metadata_links
    LOOP
        EXECUTE format('COMMENT ON TABLE %I.%I IS %L', 
                      r.schema_name, 
                      r.table_name, 
                      'Voir métadonnées dans ' || r.metadata_path);
        counter := counter + 1;
    END LOOP;
    
    UPDATE admin.metadata_links SET last_sync = CURRENT_TIMESTAMP;
    
    RETURN counter;
END;
$$ LANGUAGE plpgsql;

-- Fonction pour obtenir les informations sur une table et son fichier de métadonnées
CREATE OR REPLACE FUNCTION admin.get_table_metadata_info(p_schema VARCHAR, p_table VARCHAR)
RETURNS TABLE(
    schema_name VARCHAR,
    table_name VARCHAR,
    metadata_path VARCHAR,
    table_comment TEXT,
    last_sync TIMESTAMP
) AS $$
BEGIN
    RETURN QUERY
    SELECT 
        ml.schema_name,
        ml.table_name,
        ml.metadata_path,
        obj_description((p_schema || '.' || p_table)::regclass) as table_comment,
        ml.last_sync
    FROM 
        admin.metadata_links ml
    WHERE 
        ml.schema_name = p_schema
        AND ml.table_name = p_table;
END;
$$ LANGUAGE plpgsql;
